scene end {
    action init {
        load_image "background" "end/background.png"
        load_audio "background" "end/background.wav"
    }

    action first_come {
        set_background "background"
        play "background" loop

        set_text "Вы прошли демо уровень. А спонсор этого демо: Azino 777 - зарегистрируйся и получи 777 игровых рублей на счет."
    }
}